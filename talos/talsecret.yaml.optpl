  secretboxencryptionsecret: "op://cluster-phoebe/talos/secrets/secretboxencryptionsecret"
trustdinfo:
  token: "op://cluster-phoebe/talos/trustdinfo/token"
certs:
  etcd:
    crt: "op://cluster-phoebe/talos/certs-etcd/crt"
    key: "op://cluster-phoebe/talos/certs-etcd/key"
  k8s:
    crt: "op://cluster-phoebe/talos/certs-k8s/crt"
    key: "op://cluster-phoebe/talos/certs-k8s/key"
  k8saggregator:
    crt: "op://cluster-phoebe/talos/certs-k8saggregator/crt"
    key: "op://cluster-phoebe/talos/certs-k8saggregator/key"
  k8sserviceaccount:
    key: "op://cluster-phoebe/talos/certs-k8sserviceaccount/key"
  os:
    crt: "op://cluster-phoebe/talos/certs-os/crt"
    key: "op://cluster-phoebe/talos/certs-os/key"
